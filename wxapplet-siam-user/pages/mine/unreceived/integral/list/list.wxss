page {
  background: #f5f5f5;
  width: 100%;
  height: 100%;
  margin: 0;
}

.lists-class{
  background-color: white;
  margin: 20rpx;
  padding: 20rpx;
  border-radius: 15rpx;
}

.orderNo{
  font-size: 28rpx;
}

.denominationSalePrice{
  font-size: 30rpx;
  font-weight: bold;
}

.list-top-class{
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
}

.pay-success{
  color: #73c991;
}

.pay-fail{
  color: red;
}

.createTime{
  font-size: 26rpx;
  color: #959595;
}